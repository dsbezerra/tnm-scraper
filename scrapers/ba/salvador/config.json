{
  "baseUrl": "http://www.compras.salvador.ba.gov.br/",
  "startUrl": "http://www.compras.salvador.ba.gov.br/novo/",
  "selectors": {
    "byElement": false,
    "container": "#licitacoes",
    "modality": "body > div.quem_somos > div > div:nth-child(5) > p",
    "number": "body > div.quem_somos > div > div:nth-child(6) > p",
    "agency": "body > div.quem_somos > div > div:nth-child(8) > p",
    "description": "body > div.quem_somos > div > div:nth-child(11) > p",
    "date": [
      "body > div.quem_somos > div > div.grid_8 > p",
      "body > div.quem_somos > div > div:nth-child(5) > p"
    ],
    "link": "body > div.quem_somos > div > div:nth-child(13) > p > a"
  },
  "patterns": {
    "modality": "(?![Modalidade:\\ ]).*",
    "description": "(?![Objeto:\\ ]).*",
    "date": "\\d{2}\/\\d{2}\\/\\d{2,4}",
    "agency": "((?![Órgão Licitante:\\ ])).*",
    "number": "\\d{3,}\\/\\d{4}"
  }
}